<header class="border-bottom mb-4 sticky-top">
  <nav class="navbar navbar-expand-sm navbar-dark bg-dark">
    <div class="container">
      <% if (url.includes('/arena/login') || url.includes('/arena/dashboard')) {
      %>
      <a class="navbar-brand" href="/">Panel Game</a>
      <% } else { %>
      <a class="navbar-brand" href="/">Panel Admin</a>
      <% } %>
      <button
        class="navbar-toggler"
        type="button"
        data-bs-toggle="collapse"
        data-bs-target="#navbarSupportedContent"
        aria-controls="navbarSupportedContent"
        aria-expanded="false"
        aria-label="Toggle navigation"
      >
        <span class="navbar-toggler-icon"></span>
      </button>

      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav ms-auto">
          <li class="nav-item">
            <% if (url.includes('/login') || url.includes('/register') ||
            url.includes('/arena/dashboard') || url.includes('/arena')) { %>
            <a href="/login" class="nav-link" aria-current="page">Login</a>
          </li>
          <% } else { %>
          <li class="nav-item">
            <% if (url.includes('/histori')) { %>
            <a href="/histori" class="nav-link active" aria-current="page"
              >Histories Game</a
            >
            <% } else { %>
            <a href="/histori" class="nav-link">Histories Game</a>

            <% } %>
          </li>

          <li class="nav-item">
            <% if (url.includes('/gameusers')) { %>
            <a href="/gameusers" class="nav-link active" aria-current="page"
              >Users Game</a
            >
            <% } else { %>
            <a href="/gameusers" class="nav-link">Users Game</a>

            <% } %>
          </li>

          <li class="nav-item">
            <% if (url.includes('/histories')) { %>
            <a href="/histories" class="nav-link active" aria-current="page"
              >Histori Player
            </a>
            <% } else { %>
            <a href="/histories" class="nav-link">Histori player</a>

            <% } %>
          </li>

          <li class="nav-item">
            <a href="/login" class="nav-link" aria-current="page">Logout</a>
          </li>
          <% } %>
        </ul>
      </div>
    </div>
  </nav>
</header>
